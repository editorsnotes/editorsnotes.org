[Unit]
Description=Editors' Notes uWSGI server for test.workingnotes.org

[Service]
ExecStart=/usr/sbin/uwsgi --uid ryanshaw --gid ryanshaw --ini /etc/uwsgi/sites/test.workingnotes.org.ini

Restart=always
Type=notify
NotifyAccess=all

BindsTo=test.workingnotes.org.target

[Install]
WantedBy=multi-user.target