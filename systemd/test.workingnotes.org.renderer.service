[Unit]
Description=Editors' Notes renderer node server for test.workingnotes.org

[Service]
ExecStart= /usr/local/projects/test.workingnotes.org/lib/iojs-current/bin/iojs /usr/local/projects/test.workingnotes.org/renderer/releases/current/src/server
Restart=always

StandardOutput=syslog
StandardError=syslog
SyslogIdentifier=test.workingnotes.org.renderer

Environment= "EDITORSNOTES_API_URL=http://test.workingnotes.org" "EDITORSNOTES_CLIENT_PORT=15023"



[Install]
WantedBy=multi-user.target